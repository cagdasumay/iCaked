@model EventApp.Models.Map
@using EventApp.Models


@{
    Map map = (Map)Session["deletemap"];

    List<Map> MapList = (List<Map>)Session["MpsList"];
}

<script src="~/scripts/jquery-1.10.2.min.js"></script>

<script type="text/javascript">

        $(document).ready(function () {
            $("#id").change(function () {
                var type = $("#id").val();
                $("#back-btn").attr("href", "/Map/Delete?id=" + type);
            });

            setTimeout(function () { $(this).scrollTop(0); }, 1);

        });

</script>

<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">
        <!-- BEGIN PAGE HEADER-->
        <!-- BEGIN PAGE BAR -->
        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="index.html">Maps</a>  
                </li>
            </ul>
        </div>
        <!-- END PAGE BAR -->
        <!-- BEGIN PAGE TITLE-->
        <h1 class="page-title">
            Maps
            <small>Delete @map.Name</small>
        </h1>
        <!-- END PAGE TITLE-->
        <!-- END PAGE HEADER-->
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN EXAMPLE TABLE PORTLET-->
                <div class="portlet light portlet-fit bordered">


                    <div class="portlet-body">
                        <div>
                            <div class="dl-horizontal" style="margin-bottom: 10px;"> Are you sure to delete <b>@map.Name</b>?</div>
                            


                            @using (Html.BeginForm())
            {
                                @Html.AntiForgeryToken()

                                <div class="form-actions no-color">
                                    <a href="/Map/MapCreate" id="continue-btn" class="btn btn-default green">Back</a>
                                    <input type="submit" value="Delete" class="btn btn-default" />
                                </div>
                            }
                        </div>

                    </div>
                </div>
                <!-- END EXAMPLE TABLE PORTLET-->
            </div>
        </div>
    </div>
    <!-- END CONTENT BODY -->
</div>


