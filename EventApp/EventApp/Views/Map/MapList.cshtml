@model IEnumerable<EventApp.Models.Old_Map>
@{
    ViewBag.Title = "Maps";
}
<div class="page-content-wrapper">
    <!-- BEGIN CONTENT BODY -->
    <div class="page-content">

        <div class="page-bar">
            <ul class="page-breadcrumb">
                <li>
                    <a href="index.html">Maps</a>
                </li>
            </ul>
        </div>
        <!-- END PAGE BAR -->
        <!-- BEGIN PAGE TITLE-->
        <h1 class="page-title">
            Map List
        </h1>

        <div class="row">
                <div class="portlet light portlet-fit bordered" style="background-color: #f0f0f0">
                    <div class="portlet-body" style="padding-top: 0px !important;">
                        <div class="mt-element-list">
                            <div class="mt-list-container list-simple" style="background-color: #ffffff">
                                <ul>
                                    @foreach (var item in Model)
                                    {

                                        <li class="mt-list-item">
                                            <div class="list-icon-container">
                                                <i class="icon-map"></i>
                                            </div>

                                            <a href="@Url.Action("MapDetails","Map",new { mapdet = item.Id })" class="list-icon-container done pull-right">
                                                <i class="icon-arrow-right" style="color: #36c6d3"></i>
                                            </a>
                                            @*<div class="list-icon-container done pull-right">
                                                <i class="icon-arrow-right" style="color: #36c6d3"></i>
                                            </div>*@

                                            <div class="list-item-content">
                                                <h3 class="uppercase">
                                                    <a href="@Url.Action("MapDetails","Map",new { mapdet = item.Id })">@Html.DisplayFor(modelItem => item.Name)</a>
                                                </h3>
                                            </div>
                                        </li>
                                    }
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
        </div>

        <!-- END : LISTS -->
    </div>
    <!-- END CONTENT BODY -->
</div>


<script src="../assets/global/plugins/jquery.min.js" type="text/javascript"></script>
<script src="../assets/global/plugins/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
<script src="../assets/global/plugins/js.cookie.min.js" type="text/javascript"></script>
<script src="../assets/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js" type="text/javascript"></script>
<script src="../assets/global/plugins/jquery.blockui.min.js" type="text/javascript"></script>
<script src="../assets/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js" type="text/javascript"></script>
<!-- END CORE PLUGINS -->
