@using System.Data
@using Cake.Controllers
@using Cake.Models
@using System.Web.Optimization

@{
    ViewBag.Title = "iCaked – You Cake It, We Bake It";
    ViewBag.Description = "Make your dream cake real and order from the bakery you want.";
    ViewBag.Keywords = "cake, design cake, order online cake, online cake design, birthday cake, bakery products, party products";
    Layout = "/Views/Shared/_Layout.cshtml";
    DatabaseController dc = new DatabaseController();
    UtilityController uc = new UtilityController();

    String code = (string)Session["city"]; String searchStr = "";

    DataTable bakeries = dc.MemoryCacheByQuery("select * from ForeignBakeries where Province like '%" + code + "%'");
    DataTable partners = dc.MemoryCacheByQuery("select * from ForeignBakeries");

    for (int i = 0; i < bakeries.Rows.Count; i++)
    {
        searchStr = searchStr + " BakeryID = '" + new Guid(bakeries.Rows[i]["BakeryID"].ToString()) + "' or ";
    }

    searchStr = searchStr.Substring(0, searchStr.Length - 4);

    List<DataRow> newProducts = dc.ArrangeProductsByBakery(dc.MemoryCacheByQuery("select * from MadeCakes where Visible = 'yes' order by newid()"));
    List<DataRow> promotionProducts = dc.ArrangeProductsByBakery(dc.MemoryCacheByQuery("select * from Products where Approved = 'yes' and Priority = '2' and (" + searchStr + ")"));

    String sehir = (string)Session["city"];
}

