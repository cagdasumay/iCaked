        $(".index-promotion-products-slider").owlCarousel({
            items: 2,
            loop: true,
            itemsTablet: [600, 1],
            itemsMobile: [479, 1],
            pagination: true,
            navigation: true,
            navigationText: [
              "<img src='/Images/Site/nav_left.png'  style='width:30px;' />",
              "<img src='/Images/Site/nav_right.png' style='width:30px;' />",
            ]
        });

        var newProducts = $(".new-product-rating");

        for (i = 0; i < newProducts.length; i++) {
            $(newProducts[i]).rateYo({
                rating: parseFloat($(newProducts[i]).attr("data-rating")),
                starWidth: "15px"
            });
        }

        var swiper = new Swiper('.swiper-container', {
            pagination: '.swiper-pagination',
            slidesPerView: 'auto',
            centeredSlides: true,
            loop: true,
            paginationClickable: true,
            spaceBetween: 0,
            nextButton: '.swiper-button-next',
            prevButton: '.swiper-button-prev',
            touchEventsTarget: 'container' 
        });

    });

    function cycleImages(){
        var $active = $('#cycler .active');
        var $next = ($active.next().length > 0) ? $active.next() : $('#cycler img:first');
        $next.css('z-index',2);//move the next image up the pile
        $active.fadeOut(1500,function(){//fade out the top image
            $active.css('z-index',1).show().removeClass('active');//reset the z-index and unhide the image
            $next.css('z-index',3).addClass('active');//make the next image the top one
        });
    }

</script>

