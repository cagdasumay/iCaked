@using System.Data

@{
    List<DataTable> dt_list = (List<DataTable>)ViewData["ObjectNames"];
    DataTable objects = dt_list[5];

    List<DataRow> hobby = new List<DataRow>();
    List<DataRow> nature = new List<DataRow>();
    List<DataRow> celebration = new List<DataRow>();
    List<DataRow> baby = new List<DataRow>();
    List<DataRow> child = new List<DataRow>();
    List<DataRow> cartoon = new List<DataRow>();
    List<DataRow> food = new List<DataRow>();
    List<DataRow> job = new List<DataRow>();
    List<DataRow> other = new List<DataRow>();

    for (int i = 0; i < objects.Rows.Count; i++)
    {
        DataRow objectRow = objects.Rows[i];
        if (objectRow["Category"].ToString().Contains("Hobi")) { hobby.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Doğa")) { nature.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Kutlama")) { celebration.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Bebek")) { baby.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Çocuk")) { child.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Çizgi Film")) { cartoon.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Yiyecek")) { food.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Meslek")) { job.Add(objectRow); }
        if (objectRow["Category"].ToString().Contains("Diğer")) { other.Add(objectRow); }
    }
}

<div id="object2-all-dropdown" class="object2d-dropdown" style="padding: 0px; padding-top: 0px;">
    <div class="mobile-obj-up-btn mobile-nav" style="border-top:1px solid #cccccc;">
        <img src="~/Images/Site/up.png" />
    </div>
    <div class="draggable-div scroll obj-draggable">
        @{
            for (int i = 0; i < objects.Rows.Count; i++)
            {
                String img_str = "/Objects/Images/" + objects.Rows[i]["ImagePath"];
                <div data-is2D="true" class="add-obj category-object2d-draggable" data-bakery="@objects.Rows[i]["BakeryID"]" data-minSize="@objects.Rows[i]["MinSize"]" data-maxSize="@objects.Rows[i]["MaxSize"]" data-stepSize="@objects.Rows[i]["StepSize"]" data-obj="@objects.Rows[i]["ObjectName"]" data-keywords="@objects.Rows[i]["Keywords"]" style="background:url('@img_str');">
                </div>
            }
        }
    </div>
    <div class="mobile-obj-down-btn mobile-nav" style="border-top:1px solid #cccccc;">
        <img src="~/Images/Site/down.png" />
    </div>
</div>

