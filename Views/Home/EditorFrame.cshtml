        h1 = "Pasta Tasarıma Hoşgeldiniz !";
    }
    else
    {
        DataRow tasarim = dc.DBQueryGetter("select * from MadeCakes where MadeID = '" + new Guid(design) + "'").Rows[0];
        ViewBag.Title = "Pasta Tasarla – " + tasarim["Name"] + " Siparişi - Butik Pasta";
        ViewBag.Description = "iCaked’in 3 boyutlu editörü ile kendi pastanızı tasarlayın. 3 boyutlu tasarım " + tasarim["Name"] + " pastasını sipariş verin.";
        ViewBag.Keywords = tasarim["Name"] + " siparişi, " + tasarim["Name"] + " tasarım pasta, " + tasarim["Name"] + " pastası, online pasta tasarımı, pasta tasarımı , online pasta tasarlama";
        h1 = tasarim["Name"] + " tasarımını sipariş vermek için tıklayın.";
    }

    bool alreadySaved = String.IsNullOrEmpty(design) == false;

    bool isLogin = (Session["userinfo"] != null);

