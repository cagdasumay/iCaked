@{
    ViewBag.Title = "iCaked - Doğum Günü";
    Layout = "/Views/Shared/_LendingLayout.cshtml";
}

<!doctype html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <title>iCaked - Doğum Günü Pastaları</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Lending/Dogumgunu/css/icons.min.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Lending/Dogumgunu/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Lending/Dogumgunu/css/flexslider.min.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Lending/Dogumgunu/css/theme-velvet.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Lending/Dogumgunu/css/custom.css" rel="stylesheet" type="text/css" media="all" />
    <link href='https://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic' rel='stylesheet' type='text/css'>
    <link href="https://fonts.googleapis.com/css?family=Arimo:400,700,400italic" rel="stylesheet" type="text/css">
    <link href="~/Lending/Dogumgunu/css/font-helvetica.css" rel="stylesheet" type="text/css">
</head>
<body>

    <div class="nav-container">
        <nav class="nav-1">
            <div class="navbar">
                <div class="container">
                    <div class="row">
                        <div class="col-md-3 col-sm-6 col-xs-4">
                            <a href="https://www.icaked.com">
                                <img class="logo" alt="doğum günü logo" src="~/Lending/Dogumgunu/img/icaked.png">
                            </a>
                        </div>

                        <div class="col-md-3 text-right col-sm-6 col-md-push-6 col-xs-8">
                            <a class="btn btn-filled" href="https://www.icaked.com/Editor">Kendin Tasarla »</a>
                            <div class="mobile-toggle">
                                <div class="upper"></div>
                                <div class="middle"></div>
                                <div class="lower"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </nav>
    </div>

    <div class="main-container">
        <section class="header header-4">
            <div class="hero-slider">
                <ul class="slides">
                    <li>
                        <div class="background-image-holder">
                            <img alt="Background Image" class="background-image" src="~/Lending/Dogumgunu/img/yilbasi-bg.jpg">
                        </div>
                    </li>
                </ul>
            </div>

            <div class="container">
                <div class="row">
                    <div class="col-sm-12 text-center">
                        <h3>Hazır Tasarımlardan Seç ya da Kendin Tasarla!</h3>
                        <h5>iCaked size şuana kadar yapılmış yüzlerce konsept pasta tasarımlarını sunarken size özel kendiniz tasarlayacağınız pastanızı gerçeğe dönüştürmenize imkan sağlar.</h5>
                        <a class="btn btn-filled super-action" href="https://www.icaked.com/Editor">Pastanı Kendin Tasarla</a>
                    </div>
                </div>
            </div>
        </section>

        <section class="features features-2 bg-secondary">
            <div class="container">
                <div class="row">
                    <div class="col-md-10 col-md-offset-1 col-sm-12 text-center">
                        <h4>2016 Yılında En Çok Sipariş Verilen Tasarımlar</h4>
                        <p>Sizin için seçtiğimiz özel tasarımlardan birisini anında sipariş verebilirsiniz.</p>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-4 text-center feature">
                        <a href="https://www.icaked.com/pastane/liva/pasta/altin-ozel-pasta-09561848">
                            <img alt="altın özel pasta liva" src="~/Lending/Dogumgunu/img/pasta1.jpg">
                        </a>
                        <h5>Altın Özel Pasta</h5>
                        <p>
                            Bu yıl en çok sipariş verilen tasarım: altın özel pasta! Gözalıcı görünümü ve çikolatalı lezzetiyle doğum günlerinizin vazgeçilmezi olacak.
                        </p>
                        <a class="btn btn-filled" href="https://www.icaked.com/pastane/liva/pasta/altin-ozel-pasta-09561848">Satın Al</a>
                    </div>

                    <div class="col-sm-4 text-center feature">
                        <a href="https://www.icaked.com/pastane/liva/pasta/batman-ve-superman-ozel-pasta-920e72fa">
                            <img alt="batman vs superman pasta liva" src="~/Lending/Dogumgunu/img/pasta2.jpg">
                        </a>
                        <h5>Batman vs Superman Pasta</h5>
                        <p style="height:72px;">
                            Sizce de bu rekabet fazla uzun sürmedi mi? Doğum gününüz ile buna son verelim!
                        </p>
                        <a class="btn btn-filled" href="https://www.icaked.com/pastane/liva/pasta/batman-ve-superman-ozel-pasta-920e72fa">Satın Al</a>
                    </div>

                    <div class="col-sm-4 text-center feature">
                        <a href="https://www.icaked.com/pastane/liva/pasta/hollywood-ozel-pasta-f791bb40">
                            <img alt="sinema tutkunu özel pasta liva" src="~/Lending/Dogumgunu/img/pasta3.jpg">
                        </a>
                        <h5>Sinema Tutkunu Özel Pasta</h5>
                        <p>
                            Sinema tutkunlarına özel tasarlanan pasta ile doğum günlerinden ince işçilik barındıran bu pasta ile taçlandırın!
                        </p>
                        <a class="btn btn-filled" href="https://www.icaked.com/pastane/liva/pasta/hollywood-ozel-pasta-f791bb40">Satın Al</a>
                    </div>
                </div>
            </div>
        </section>

        <section class="features features-15">
            <div class="row">
                <div class="col-sm-4 col-xs-12 text-center feature">
                    <a href="https://www.icaked.com/Editor" style="float:left;">
                        <i class="icon-picture-streamline-1"></i>
                        <h5 class="text-white">Pastanızı Tasarlayın</h5>
                        <p class="text-white">
                            Eğer hazır tasarım pastalarımız içinize sinmediyse kendiniz online editörümüzü kullanarak 3 boyutlu pastanızı tasarlayabilirsiniz.
                        </p>
                    </a>
                </div>

                <div class="col-sm-4 col-xs-12 text-center feature">
                    <a href="https://www.icaked.com/pastane" style="float:left;">
                        <i class="icon-dashboard-speed-streamline"></i>
                        <h5 class="text-white">Pastanenizi Seçin</h5>
                        <p class="text-white">
                            Tasarladığınız pastayı anlaşmalı olduğumuz pastanelerden birisini seçerek yaptırtabilirsiniz.
                        </p>
                    </a>
                </div>

                <div class="col-sm-4 col-xs-12 text-center feature">
                    <a href="https://www.icaked.com/pastane" style="float:left;">
                        <i class="icon-ink-pen-streamline"></i>
                        <h5 class="text-white">Sipariş Verin</h5>
                        <p class="text-white">
                            Tasarladığınız pastayı belirlediğiniz pastanede yaptırmak için sipariş verin.
                        </p>
                    </a>
                </div>
            </div>
        </section>

        <section class="features features-1">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 text-center">
                        <ul class="hover-gallery" speed="5000">
                            <li>
                                <a href="https://www.icaked.com/pastane/liva/pasta/kopuk-banyosu-ozel-pasta-412e441e">
                                    <img alt="köpük banyosu özel pasta" src="~/Lending/Dogumgunu/img/pasta5.jpg">
                                </a>
                            </li>
                            <li class="active">
                                <a href="https://www.icaked.com/pastane/liva/pasta/cilekli-ozel-pasta-118a1d81">
                                    <img alt="çilekli özel pasta liva" src="~/Lending/Dogumgunu/img/pasta4.jpg">
                                </a>
                            </li>
                            <li>
                                <a href="https://www.icaked.com/pastane/liva/pasta/sihirli-dokunus-ozel-pasta-967688c9">
                                    <img alt="sihirli dokunuş özel pasta" src="~/Lending/Dogumgunu/img/pasta6.jpg">
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 text-center">
                        <h3>Özel Pastalarımızı İncelediniz mi?</h3>
                        <p class="lead">
                            Şuana kadar yapılan onlarca özel pasta tasarımlarımızı inceleyin, beğendiniz tasarımı kendinize uygun kişiselleştirerek sipariş verin!
                        </p>
                        <a class="btn btn-filled" href="https://www.icaked.com/pastane/liva">Tüm Pastaları Görüntüle</a>
                    </div>
                </div>
            </div>
        </section>

        <section class="contact contact-3 bg-secondary">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 text-center">
                        <h3>Bize tarif etmek ister misiniz?</h3>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1">
                        <form class="form-email" data-success="İsteğiniz başarıyla alınmıştır, sizinle iletişime geçeceğiz." data-error="Lütfen tüm alanları doldurun.">
                            <input type="text" name="name" class="signup-name-field validate-required" placeholder="İsminiz">
                            <input type="text" name="email" class="validate-required validate-email signup-email-field" placeholder="Mail Adresiniz">
                            <textarea rows="4" name="message" class="validate-required" placeholder="İstediğiniz pastanın tarifini yapınız"></textarea>
                            <input type="submit" value="Özel Pasta İstek Gönder »">
                            <p class="sub">
                                * Kişisel bilgilerinizi kesinlikle kayıt altına almıyoruz.
                            </p>
                            <iframe srcdoc="" class="mail-list-form"></iframe>
                        </form>
                    </div>
                </div>
            </div>
        </section>
        <footer class="footer footer-5">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12 text-center">
                        <img alt="doğumgünü logo" src="~/Lending/Dogumgunu/img/icaked.png">
                        <p class="lead">
                            Sorularınız mı var? Hemen bizi arayın.
                        </p>
                        <a class="btn btn-filled" href="tel:908502500458">0 (850) 250 04 58</a>
                    </div>
                </div>

                <div class="row">
                    <div class="col-sm-12 text-center">
                        <div class="lower">
                            <p class="sub">
                                © Copyright 2016 iCaked.com - All Rights Reserved
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <script src="~/Lending/Dogumgunu/js/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/TweenMax.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/gsap/latest/plugins/ScrollToPlugin.min.js"></script>
    <script src="~/Lending/Dogumgunu/js/bootstrap.min.js"></script>
    <script src="~/Lending/Dogumgunu/js/flexslider.min.js"></script>
    <script src="~/Lending/Dogumgunu/js/placeholders.min.js"></script>
    <script src="~/Lending/Dogumgunu/js/parallax.js"></script>
    <script src="~/Lending/Dogumgunu/js/scripts.js"></script>
</body>
</html>